<query id="getAdminUsers" action="select">
	<tables>
		<table name="member" />
	</tables>
	<columns>
		<column name="member_srl" />
	</columns>
	<conditions>
		<condition operation="equal" column="is_admin" default="Y" />
	</conditions>
	<navigation>
		<index var="sort_index" default="member_srl" order="desc" />
	</navigation>
</query>
