<?xml version="1.0" encoding="UTF-8"?>
<widget version="0.2">
	<title xml:lang="ko">현재 접속자 출력 위젯</title>
	<description xml:lang="ko">사이트에 접속한 사용자를 출력하는 위젯입니다.</description>
	<version>0.3.2</version>
	<date>2013-10-13</date>

	<author email_address="admin@funnyxe.com" link="http://funnyxe.com">
		<name xml:lang="ko">퍼니엑스이</name>
	</author>

	<extra_vars>
		<var id="title">
			<type>text</type>
			<name xml:lang="ko">위젯 제목</name>
			<description xml:lang="ko">위젯 상단에 출력될 제목을 입력해주세요.</description>
		</var>
		<var id="list_count">
			<type>text</type>
			<name xml:lang="ko">목록수</name>
			<description xml:lang="ko">출력될 목록의 수를 정하실 수 있습니다. (기본 10개)</description>
		</var>
		<var id="page_count">
			<type>text</type>
			<name xml:lang="ko">페이지수</name>
			<description xml:lang="ko">출력될 페이지의 수를 정하실 수 있습니다. (기본 5개)</description>
		</var>
		<var id="only_member">
			<type>select</type>
			<name xml:lang="ko">회원만 출력하기</name>
			<options>
				<value>N</value>
				<name xml:lang="ko">아니오</name>
			</options>
			<options>
				<value>Y</value>
				<name xml:lang="ko">예</name>
			</options>
		</var>
		<var id="exclude_admin">
			<type>select</type>
			<name xml:lang="ko">관리자 제외</name>
			<options>
				<value>N</value>
				<name xml:lang="ko">아니오</name>
			</options>
			<options>
				<value>Y</value>
				<name xml:lang="ko">예</name>
			</options>
		</var>
		<var id="show_user_count">
			<type>select</type>
			<name xml:lang="ko">접속자 수 출력</name>
			<options>
				<value>N</value>
				<name xml:lang="ko">아니오</name>
			</options>
			<options>
				<value>Y</value>
				<name xml:lang="ko">예</name>
			</options>
			<description xml:lang="ko">접속자 수를 출력합니다.</description>
		</var>
	</extra_vars>
</widget>
